<!---formulario producto --->

<h1 style ="color:blue">Ingrese Productos formulario</h1>

<form id="form1">

    <div class="form-group">
        <label for="code">Codigo</label>
        <input id="code" class="form-control" type="text" name="code">
    </div>
     <div class="form-group">
        <label for="title">title</label>
        <input id="title" class="form-control" type="text" name="title">
    </div>
    <div class="form-group">
        <label for="description">Descripcion</label>
        <input id="description" class="form-control" type="text" name="description">
    </div>
    <div class="form-group">
        <label for="price">price</label>
        <input id="price" class="form-control" type="text" name="price">
    </div>
    <div class="form-group">
        <label for="category">category</label>
        <input id="category" class="form-control" type="text" name="category">
    </div>
   <div class="form-group">
        <label for="stock">stock</label>
        <input id="stock" class="form-control" type="text" name="stock">
    </div>
    <input type="submit" class="btn btn-warning" id="btn" >Crear</button>
</form>


